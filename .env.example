# API Configuration
# If VITE_API_URL is not set, the app will use mock data
VITE_API_URL=https://bricksure-api.onrender.com

# Environment
VITE_ENV=development

# Encryption keys (matching backend environment variable names)
VITE_ENCRYPTION_SERVICE_KEY=bricksure-encryption-key-2024-secure
VITE_IV_KEY=bricksure-iv-key16

# Google Maps API Key (for Places Autocomplete)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
